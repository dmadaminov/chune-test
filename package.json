{
  "name": "chune",
  "version": "0.1.0",
  "description": "Chune is a personalized music news feed featuring articles from a variety of music magazines as well as artist interviews from hundreds of curated YouTube channels",
  "engines": {
    "node": "8.11.2"
  },
  "main": "index.js",
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "jest",
    "server": "npm run build && nodemon server.js -e html,js,scss --ignore dist"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "3.0.1",
    "@material-ui/icons": "3.0.1",
    "axios": "0.18.0",
    "body-parser": "1.16.1",
    "compression": "1.7.1",
    "dotenv": "6.0.0",
    "express": "4.14.1",
    "geodist": "0.2.1",
    "history": "4.6.3",
    "lodash": "4.17.10",
    "mixpanel-browser": "2.22.4",
    "moment": "2.21.0",
    "morgan": "1.8.1",
    "node-spotify-api": "1.0.7",
    "prop-types": "15.5.8",
    "react": "16.5.0",
    "react-autosuggest": "9.3.4",
    "react-dom": "16.5.0",
    "react-materialize": "1.1.2",
    "react-mixpanel": "0.0.11",
    "react-redux": "5.0.7",
    "react-redux-geolocation": "1.0.14",
    "react-responsive": "4.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "4.1.1",
    "react-spotify-player": "1.0.4",
    "react-tap-event-plugin": "3.0.3",
    "react-tweet-embed": "1.1.1",
    "react-waypoint": "8.0.2",
    "react-youtube": "7.5.0",
    "redux": "3.6.0",
    "redux-devtools-extension": "2.13.2",
    "redux-logger": "2.8.1",
    "redux-persist": "^5.10.0",
    "redux-saga": "0.16.0",
    "redux-thunk": "2.2.0",
    "request": "2.85.0",
    "reselect": "3.0.1",
    "video-react": "0.13.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "autoprefixer": "7.1.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.2",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "eslint": "5.5.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-loader": "2.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "nodemon": "^1.18.4",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "rimraf": "2.6.2",
    "style-loader": "0.23.0",
    "url-loader": "1.1.1",
    "webpack": "4.17.2",
    "webpack-dev-server": "3.1.8"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testMatch": [
      "<rootDir>/__tests__/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testURL": "http://localhost",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
      "browser": true
    },
    "rules": {
      "comma-dangle": "off",
      "import/no-default-export": "error",
      "import/prefer-default-export": "off",
      "function-paren-newline": "off",
      "react/jsx-one-expression-per-line": "off",
      "max-len": [
        "error",
        {
          "code": 100,
          "tabWidth": 2,
          "ignoreUrls": true,
          "ignoreStrings": true,
          "ignoreTemplateLiterals": true,
          "ignoreRegExpLiterals": true
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "__tests__/**/*.@(js|jsx)"
        ],
        "env": {
          "jest": true,
          "node": true
        },
        "rules": {
          "react/jsx-filename-extension": "off"
        }
      }
    ]
  }
}
